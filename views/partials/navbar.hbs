                          <input name="startDate" type="text" id="datepicker" class="mx-sm-3 form-control" placeholder="Start Date">
                          <input type="hidden" name="showNav" value="true">
                        </div>
          
                        <div class="form-group">
                          <label for="inlineFormInputGroup" class="sr-only">End Date</label>
                          <input name="endDate" type="text" id="datepicker2" class="mx-sm-3 form-control" placeholder="End Date">
                        </div>

                        <div class="form-group">
                          <label for="inlineFormInputGroup" class="sr-only">Year</label>
                          <input name="year" type="number" class="mx-sm-3 form-control" placeholder="Year">
                        </div>
          
                        <div class="form-group">
                          <label for="inlineFormInputGroup" class="sr-only">End Date</label>
                          <select class="mx-sm-3 form-control" id="exampleSelect1" name="semester">
                            <option value="None">Semister</option>
                            <option value="Summer">Summer</option>
                            <option value="Spring">Spring</option>
                            <option value="Fall">Fall</option>
                          </select>
                        </div>
                        <br>
                        <div class="form-group">
                          <button type="button" class="mx-sm-3 btn btn-danger" data-dismiss="modal">Cancel</button>
                          <button type="submit" value="Submit" class="btn btn-success">Submit</button>
                        </div>
                
                </div>
              </div>
            </div>
          </section>
        </div>
      </form>

    </div>
